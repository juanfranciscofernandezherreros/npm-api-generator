curl -X POST "http://localhost:8080/movies/filter?page=0&size=10" -H "Content-Type: application/json" -d '{
    "title": {
        "like": "Matrix"
    }
}'

---
curl -X POST "http://localhost:8080/movies/groupBy?field=genre" -H "Content-Type: application/json" -d '{
    "rating": {
        "gt": 8.0
    }
}'
